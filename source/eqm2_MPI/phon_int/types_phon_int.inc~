      TYPE level_typ

        sequence

        integer                 ::npr
        integer                 :: n
        integer                 :: l
        integer                 :: j         ! 2*j        
        double precision        :: en        ! s.p.-energy
      
      END TYPE level_typ

      TYPE amp_typ

        sequence 

        integer                 :: par        
        integer                 :: hol      
        double precision        :: am

      END TYPE amp_typ

      TYPE rho_typ

        sequence 

        integer                 :: ilap
        integer                 :: j
        integer                 :: i1        
        integer                 :: i2      
        double precision        :: ro

      END TYPE rho_typ



     

      
